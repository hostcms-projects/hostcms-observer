{
    "name": "maximzasorin/hostcms-observer",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Zasorin",
            "email": "maximzasorin@gmail.com"
        }
    ],
    "repositories": {
       "hostcms-framework": {
            "type": "package",
            "package": {
                "name": "maximzasorin/hostcms-framework",
                "version": "6.7.1",
                "source": {
                    "url": "https://github.com/maximzasorin/hostcms-framework",
                    "type": "git",
                    "reference": "6.7.1"
                },
                "autoload": {
                    "classmap": ["modules/"]
                }
            }
        },
        "hostcms-testing": {
            "type": "git",
            "url": "git@github.com:maximzasorin/hostcms-testing.git"
        }
    },
    "require": {
        "maximzasorin/hostcms-framework": "*",
        "maximzasorin/hostcms-testing": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    }
}